using Nemerle;
using Nemerle.Collections;
using Nemerle.Text;
using Nemerle.Utility;

using System;
using System.Collections.Generic;
using System.Linq;

namespace MiniC.Compiler
{
  module OptionExtensions
  {
    public Fill[T](this m: option[T], f: void -> T): T
    {
      match (m)
      {
        | Some(x) => x
        | None => f()
      }
    }
  }
}